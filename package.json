{
	"name": "underlay-r1",
	"version": "0.0.1",
	"description": "r1: an Underlay registry",
	"author": "Knowledge Futures, Inc <hello@knowledgefutures.org>",
	"license": "GPL-2.0+",
	"sideEffects": false,
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "NODE_ENV=production node server.js",
		"format": "prettier --write '*.{ts,tsx,js,jsx,scss,json}'",
		"type-check": "tsc",
		"analyze": "cross-env ANALYZE=true next build"
	},
	"engines": {
		"node": ">=14.16.1"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,scss,json}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@blueprintjs/core": "^4.0.0-beta.10",
		"@blueprintjs/icons": "^4.0.0-beta.6",
		"@hapi/iron": "^6.0.0",
		"@next/bundle-analyzer": "^12.0.7",
		"@prisma/client": "^3.6.0",
		"classnames": "^2.3.1",
		"cookie": "0.4.1",
		"cross-env": "^7.0.3",
		"crypto-js": "^4.0.0",
		"diacritics": "^1.3.0",
		"fathom-client": "^3.2.0",
		"next": "^12.0.4",
		"next-connect": "^0.11.0",
		"next-s3-upload": "^0.1.7",
		"next-superjson": "^0.0.2",
		"normalize-css": "^2.3.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"superjson": "^1.7.5",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@types/cookie": "^0.4.0",
		"@types/crypto-js": "^4.0.1",
		"@types/diacritics": "^1.3.1",
		"@types/node": "^14.0.0",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.1",
		"@types/uuid": "^8.3.0",
		"lint-staged": "^12.1.2",
		"node-sass": "^6.0.1",
		"prettier": "^2.4.0",
		"prisma": "^3.6.0",
		"typescript": "^4.4.2",
		"yorkie": "^2.0.0"
	},
	"browser": {
		"./utils/server/auth/user.ts": false
	}
